{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": { "prettier/prettier": "error" }
}
